<?xml version="1.0" encoding="UTF-8"?>
<workspaceFilter version="1.0">
    <filter root="/content/dam/filevaultsample">
        <!-- Exclude folder thumbnails - they will be generated automatically -->
        <exclude pattern="^.*/jcr:content/folderThumbnail(/.*)?$"/>
    </filter>
    <filter root="/content/filevaultsample"/>
    <filter root="/content/experience-fragments/filevaultsample"/>
    <filter root="/content/cq:tags/sample"/>
    <filter root="/conf/filevaultsample">
        <include pattern="/conf/filevaultsample/.*"/>
        <exclude pattern="/conf/filevaultsample/rep:policy(/.*)?"/>
        <exclude pattern="/conf/filevaultsample/settings/wcm(/.*)?"/>
    </filter>
</workspaceFilter>
